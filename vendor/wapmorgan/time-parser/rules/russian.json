{
	"language": "Russian",
	"rules": [
		{
			"name": "time",
			"type": "absolute",
			"regex": "~(в (?<hour>\\d{1,2})\\:(?<min>\\d{2})(\\:(?<sec>\\d{2}))?)~u"
		},
		{
			"name": "weekday",
			"type": "absolute",
			"regex": "~во? (?<pronoun>этот?|эту|следующий|следующее) (?<weekday>понедельник|вторник|среду|четверг|пятницу|субботу|воскресенье|воскресение)~u"
		},
		{
			"name": "year",
			"type": "absolute",
			"regex": "~в ((?<pronoun>этот?|этом|следующем)|((?<digit>[[:digit:]]+))(-?м)?) году~u"
		},
		{
			"name": "month",
			"type": "absolute",
			"regex": "~в ((?<pronoun>этом|следующем) месяце|(?<month>январе|феврале|марте|апреле|мае|июне|июле|августе|сентябре|октябре|ноябре|декабре))~u"
		},
		{
			"name": "week",
			"type": "absolute",
			"regex": "~на (?<pronoun>этой|следующей) неделе~u"
		},
		{
			"name": "hour",
			"type": "relative",
			"regex": "~(через ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) (часов|часа))~u"
		},
		{
			"name": "minute",
			"type": "relative",
			"regex": "~(через ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) (минуту|минут|мин))~u"
		},
		{
			"name": "sec",
			"type": "relative",
			"regex": "~(через ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) (секунду|секунд|сек))~u"
		},
		{
			"name": "year",
			"type": "relative",
			"regex": "~(через ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) (лет|год))~u"
		},
		{
			"name": "week",
			"type": "relative",
			"regex": "~(через ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) (неделю|недель|недели))~u"
		},
		{
			"name": "day",
			"type": "relative",
			"regex": "~(через ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) (дней|день|дня))~u"
		},
		{
			"name": "month",
			"type": "relative",
			"regex": "~(через ((?<digit>[[:digit:]]+)|(?<alpha>[[:alpha:]]+)) (месяцев|месяца|месяц))~u"
		}
	],
	"week_days": {
		"понедельник": "monday",
		"вторник": "tuesday",
		"среду": "wednesday",
		"четверг": "thursday",
		"пятницу": "friday",
		"субботу": "saturday",
		"воскресенье": "sunday",
		"воскресение": "sunday"
	},
	"pronouns": {
		"этот": "this",
		"эту": "this",
		"следующий": "next",
		"следующую": "next",
		"следующем": "next"
	},
	"months": {
		"январе": "january",
		"феврале": "february",
		"марте": "march",
		"апреле": "april",
		"мае": "may",
		"июне": "june",
		"июле": "july",
		"августе": "august",
		"сентябре": "september",
		"октябре": "october",
		"ноябре": "november",
		"декабре": "december"
	},
	"units": {
		"один": 1,
		"одну": 1,
		"два": 2,
		"две": 2,
		"три": 3,
		"четыре": 4,
		"пять": 5,
		"шесть": 6,
		"семь": 7,
		"восемь": 8,
		"девять": 9,
		"десять": 10,
		"одиннадцать": 11,
		"двенадцать": 12,
		"тринадцать": 13,
		"четырнадцать": 14,
		"пятнадцать": 15,
		"шестнадцать": 16,
		"семнадцать": 17,
		"восемнадцать": 18,
		"девятнадцать": 19,
		"двадцать": 20
	}
}
